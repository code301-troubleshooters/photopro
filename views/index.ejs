<%- include('./components/header') %>


<main>
    <section class="main-banner">
        <div class="parallex">
        </div>
        <div class="row">
            <div class="search-form-title">
                <h3>I don’t trust words. I trust pictures</h3>
            </div>
            <div class="search-form">
                <%- include('./components/search-form') %>
            </div>
        </div>
    </section>

    <section class="potd=-section">
        <div class="row">
            <div class="section-title">
                <h3>
                    PHOTO OF THE DAY
                </h3>
                <hr>
            </div>
            <div class="potd-card">
                <div class="potd-info">
                    <div class="potd-date">
                        <h4><%= pictureOfTheDay.date %></h4>
                    </div>
                    <div class="potd-title">
                        <h3><%= pictureOfTheDay.title %></h3>
                    </div>
                    <div class="potd-caption">
                        <p><%= pictureOfTheDay.caption %></p>
                    </div>
                    <div class="potd-credit">
                        <h4><%= pictureOfTheDay.credit %></h4>
                    </div>
                </div>
                <div class="potd-img">
                    <img src="<%= pictureOfTheDay.url %>" alt="<%= pictureOfTheDay.title %>">
                </div>
            </div>
        </div>
    </section>

    <section class="about-photopro">
